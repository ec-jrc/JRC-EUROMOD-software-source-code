					<Parameter>
						<VarName>ils_dispy</VarName>
					</Parameter>
					<Parameter>
						<Name>EquivalenceScale</Name>
						<VarName>OECDScale</VarName>
						<Source>BASELINE</Source>
					</Parameter>
				</Parameters>
			</Action>
			<Action>
				<CalculationType>CalculateSumWeighted</CalculationType>
				<OutputVar>totalDispy</OutputVar>
				<FormulaString><![CDATA[DATA_VAR[@eqDispy]]]></FormulaString>
				<Reform>true</Reform>
			</Action>
			<Action>
				<CalculationType>CreateDeciles</CalculationType>
				<OutputVar>deciles_eqDispy</OutputVar>
				<Parameters>
					<Parameter>
					<Name>IncomeVar</Name>
						<VarName>eqDispy</VarName>
						</Parameter>
					<Parameter>
						<Name>GroupingVar</Name>
						<VarName>idhh</VarName>
					</Parameter>
				</Parameters>
			</Action>
			<Action>
				<CalculationType>CalculateArithmetic</CalculationType>
				<FormulaString><![CDATA[SAVED_VAR[@deciles_eqDispy~dec~5] * 0.4]]></FormulaString>
				<OutputVar>povLine40</OutputVar>
				<Reform>true</Reform>
			</Action>
			<Action>
				<CalculationType>CalculateArithmetic</CalculationType>
				<FormulaString><![CDATA[SAVED_VAR[@deciles_eqDispy~dec~5] * 0.5]]></FormulaString>
				<OutputVar>povLine50</OutputVar>
				<Reform>true</Reform>
			</Action>
			<Action>
				<CalculationType>CalculateArithmetic</CalculationType>
				<FormulaString><![CDATA[SAVED_VAR[@deciles_eqDispy~dec~5] * 0.6]]></FormulaString>
				<OutputVar>povLine60</OutputVar>
				<Reform>true</Reform>
			</Action>
			<Action>
				<CalculationType>CalculateArithmetic</CalculationType>
				<FormulaString><![CDATA[SAVED_VAR[@deciles_eqDispy~dec~5] * 0.7]]></FormulaString>
				<OutputVar>povLine70</OutputVar>
				<Reform>true</Reform>
			</Action>
			<Action>
				<CalculationType>CreateFlag</CalculationType>
				<OutputVar>IsPoor40</OutputVar>
				<Filter>
					<Name>FilterPoor40</Name>
					<FormulaString><![CDATA[DATA_VAR[@eqDispy] <= SAVED_VAR[@povLine40]]]></FormulaString>
				</Filter>
				<Reform>true</Reform>
			</Action>
			<Action>
				<CalculationType>CreateFlag</CalculationType>
				<OutputVar>IsPoor50</OutputVar>
				<Filter>
					<Name>FilterPoor50</Name>
					<FormulaString><![CDATA[DATA_VAR[@eqDispy] <= SAVED_VAR[@povLine50]]]></FormulaString>
				</Filter>
				<Reform>true</Reform>
			</Action>
			<Action>
