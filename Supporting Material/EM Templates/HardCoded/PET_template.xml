<Template>
  <TemplateInfo>
    <Name>Policy Effects</Name>
    <Title>[@MainCaption] for [baseCountry]</Title>
    <Subtitle>shown as a percentage (%) change in mean (equivalised) household disposable income by income component and income decile group</Subtitle>
    <Button>[baseCountry] [@BottomButtonCaption]</Button>
    <Description>Pet template</Description>
    <TemplateType>BaselineReform</TemplateType>
    <RequiredVariables>
      <RequiredVariable>
        <Name>weight</Name>
        <ReadVar>dwt</ReadVar>
      </RequiredVariable>
      <RequiredVariable>
        <Name>idhh</Name>
        <ReadVar>idhh</ReadVar>
      </RequiredVariable>
      <RequiredVariable>
        <Name>idperson</Name>
        <ReadVar>idperson</ReadVar>
      </RequiredVariable>
      <RequiredVariable>
        <Name>age</Name>
        <ReadVar>dag</ReadVar>
      </RequiredVariable>
      <RequiredVariable>
        <Name>ilsSicEe</Name>
        <ReadVar>ils_sicee</ReadVar>
      </RequiredVariable>
      <RequiredVariable>
        <Name>ilsSicSe</Name>
        <ReadVar>ils_sicse</ReadVar>
      </RequiredVariable>
      <RequiredVariable>
        <Name>ilsTax</Name>
        <ReadVar>ils_tax</ReadVar>
      </RequiredVariable>
      <RequiredVariable>
        <Name>ilsBenNt</Name>
        <ReadVar>ils_bennt</ReadVar>
      </RequiredVariable>
      <RequiredVariable>
        <Name>ilsBenMt</Name>
        <ReadVar>ils_benmt</ReadVar>
      </RequiredVariable>
      <RequiredVariable>
        <Name>ilsDispy</Name>
        <ReadVar>ils_dispy</ReadVar>
      </RequiredVariable>
      <RequiredVariable>
        <Name>ilsOrigy</Name>
        <ReadVar>ils_origy</ReadVar>
      </RequiredVariable>
      <RequiredVariable>
        <Name>ilsPen</Name>
        <ReadVar>ils_pen</ReadVar>
      </RequiredVariable>
    </RequiredVariables>
    <OptionalVariables>
      <OptionalVariable>
        <Name>ilsSicOt</Name>
        <ReadVar>ils_sicot</ReadVar>
      </OptionalVariable>
    </OptionalVariables>
    <UserVariables>
      <!-- user variables used for transferting constants for formula (generated per alpha per country/data) -->
      <UserVariable>
        <UserInputType>Numeric</UserInputType>
        <Name>Const1</Name>
        <DefaultValue>1</DefaultValue>
      </UserVariable>
      <UserVariable>
        <UserInputType>Numeric</UserInputType>
        <Name>Const2</Name>
        <DefaultValue>1</DefaultValue>
      </UserVariable>
      <UserVariable>
        <UserInputType>Numeric</UserInputType>
        <Name>Const3</Name>
        <DefaultValue>1</DefaultValue>
      </UserVariable>
      <!-- user variables used for transferting button captions and titles -->
      <UserVariable>
        <UserInputType>Numeric</UserInputType>
        <!-- generated once (as COUNTRY is added in the template, see above) -->
        <Name>MainCaption</Name>
        <Description>The policy effects in YYYY - YYYY for COUNTRY</Description>
        <DisplayDescription>true</DisplayDescription>
        <DefaultValue>1</DefaultValue>
      </UserVariable>
      <UserVariable>
        <UserInputType>Numeric</UserInputType>
        <!-- generated per country/data -->
        <Name>BottomButtonCaption</Name>
        <Description>DataX</Description>
        <DisplayDescription>true</DisplayDescription>
        <DefaultValue>1</DefaultValue>
      </UserVariable>
      <UserVariable>
        <!-- generated per alpha per country/data -->
        <UserInputType>Numeric</UserInputType>
        <Name>TableCaption</Name>
        <Description>Results for CC with alpha: CUSTOM/CPI/MII [ALPHA] on dataset CC_YYYY_aX</Description>
        <DisplayDescription>true</DisplayDescription>
        <DefaultValue>1</DefaultValue>
      </UserVariable>
      <UserVariable>
        <!-- generated per alpha per country/data -->
        <UserInputType>Numeric</UserInputType>
        <Name>TopButtonCaption</Name>
        <Description>alpha=CUSTOM/CPI/MII (DataX)</Description>
        <DisplayDescription>true</DisplayDescription>
        <DefaultValue>1</DefaultValue>
      </UserVariable>
    </UserVariables>
  </TemplateInfo>
  <Globals>
		<Actions>
			<Action>
        <CalculationType>CreateOECDScale</CalculationType>
				<OutputVar>OECDscale</OutputVar>
        <Reform>false</Reform>
      </Action>
			<Action>
        <CalculationType>CreateEquivalized</CalculationType>
				<OutputVar>eqDispy</OutputVar>
				<Parameters>
					<Parameter>
            <VarName>ilsDispy</VarName>
					</Parameter>
					<Parameter>
            <Reform>false</Reform>
						<Name>EquivalenceScale</Name>
            <VarName>OECDscale</VarName>
					</Parameter>
				</Parameters>
			</Action>
      <Action>
        <CalculationType>CreateDeciles</CalculationType>
        <OutputVar>deciles_eqDispy</OutputVar>
        <Reform>false</Reform>
        <Parameters>
          <Parameter>
            <Name>IncomeVar</Name>
            <VarName>eqDispy</VarName>
          </Parameter>
        </Parameters>
      </Action>
      <Action>
        <CalculationType>CreateEquivalized</CalculationType>
        <OutputVar>eqOrigy</OutputVar>
        <Parameters>
          <Parameter>
            <VarName>ilsOrigy</VarName>
          </Parameter>
          <Parameter>
            <Reform>false</Reform>
            <Name>EquivalenceScale</Name>
            <VarName>OECDscale</VarName>
          </Parameter>
        </Parameters>
      </Action>
      <Action>
        <CalculationType>CreateEquivalized</CalculationType>
        <OutputVar>eqPen</OutputVar>
        <Parameters>
          <Parameter>
            <VarName>ilsPen</VarName>
          </Parameter>
          <Parameter>
            <Reform>false</Reform>
            <Name>EquivalenceScale</Name>
            <VarName>OECDscale</VarName>
          </Parameter>
        </Parameters>
      </Action>
      <Action>
        <CalculationType>CreateEquivalized</CalculationType>
        <OutputVar>eqSicEe</OutputVar>
        <Parameters>
          <Parameter>
            <VarName>ilsSicEe</VarName>
          </Parameter>
          <Parameter>
            <Reform>false</Reform>
            <Name>EquivalenceScale</Name>
            <VarName>OECDscale</VarName>
          </Parameter>
        </Parameters>
      </Action>
      <Action>
        <CalculationType>CreateEquivalized</CalculationType>
        <OutputVar>eqSicSe</OutputVar>
        <Parameters>
          <Parameter>
            <VarName>ilsSicSe</VarName>
          </Parameter>
          <Parameter>
            <Reform>false</Reform>
            <Name>EquivalenceScale</Name>
            <VarName>OECDscale</VarName>
          </Parameter>
        </Parameters>
      </Action>
      <Action>
        <CalculationType>CreateEquivalized</CalculationType>
        <OutputVar>eqSicOt</OutputVar>
        <Parameters>
          <Parameter>
            <VarName>ilsSicOt</VarName>
          </Parameter>
          <Parameter>
            <Reform>false</Reform>
            <Name>EquivalenceScale</Name>
            <VarName>OECDscale</VarName>
          </Parameter>
        </Parameters>
      </Action>
      <Action>
        <CalculationType>CreateEquivalized</CalculationType>
        <OutputVar>eqBenMt</OutputVar>
        <Parameters>
          <Parameter>
            <VarName>ilsBenMt</VarName>
          </Parameter>
          <Parameter>
            <Reform>false</Reform>
            <Name>EquivalenceScale</Name>
            <VarName>OECDscale</VarName>
          </Parameter>
        </Parameters>
      </Action>
      <Action>
        <CalculationType>CreateEquivalized</CalculationType>
        <OutputVar>eqBenNt</OutputVar>
        <Parameters>
          <Parameter>
            <VarName>ilsBenNt</VarName>
          </Parameter>
          <Parameter>
            <Reform>false</Reform>
            <Name>EquivalenceScale</Name>
            <VarName>OECDscale</VarName>
          </Parameter>
        </Parameters>
      </Action>
      <Action>
        <CalculationType>CreateEquivalized</CalculationType>
        <OutputVar>eqTax</OutputVar>
        <Parameters>
          <Parameter>
            <VarName>ilsTax</VarName>
          </Parameter>
          <Parameter>
            <Reform>false</Reform>
            <Name>EquivalenceScale</Name>
            <VarName>OECDscale</VarName>
          </Parameter>
        </Parameters>
      </Action>
      <Action>
        <CalculationType>CalculateWeightedAverage</CalculationType>
        <FormulaString><![CDATA[OP_VAR[@AverageVar]]]></FormulaString>
        <OutputVar>averageDispy</OutputVar>
        <Parameters>
          <Parameter>
            <Name>AverageVar</Name>
            <VarName>eqDispy</VarName>
          </Parameter>
        </Parameters>
      </Action>
      <Action>
        <CalculationType>CalculateArithmetic</CalculationType>
        <OutputVar>negative</OutputVar>
        <FormulaString><![CDATA[-1.0]]></FormulaString>
      </Action>
      <Action>
        <CalculationType>CalculateArithmetic</CalculationType>
        <OutputVar>positive</OutputVar>
        <FormulaString><![CDATA[1.0]]></FormulaString>
      </Action>
    </Actions>
	</Globals>
	<Pages>
		<Page>
      <Name>PolicyEffects</Name>
      <Title><![CDATA[Results for [baseCountry] with alpha: [@TableCaption]]]></Title>
      <Button>[@TopButtonCaption]</Button>
      <PerReform>true</PerReform>
      <Tables>
				<Table>
          <Action>
            <CalculationType>CalculateWeightedAverage</CalculationType>
            <FormulaString><![CDATA[SAVED_VAR[@MySign] * ((USR_VAR[@Const1] * (USR_VAR[@Const2] * OP_VAR[@MyCounter] - OP_VAR[@MyBase]) * USR_VAR[@Const3]) / OP_VAR[@BDPI] * 100)]]></FormulaString>
            <Parameters>
              <Parameter>
                <Name>Const1</Name>
                <VarName>Const1</VarName>
              </Parameter>
              <Parameter>
                <Name>Const2</Name>
                <VarName>Const2</VarName>
              </Parameter>
              <Parameter>
                <Name>Const3</Name>
                <VarName>Const3</VarName>
              </Parameter>
              <Parameter>
                <Name>BDPI</Name>
                <VarName>eqDispy</VarName>
                <Reform>false</Reform>
              </Parameter>
              <Parameter>
                <Name>MySign</Name>
                <VarName>positive</VarName>
              </Parameter>
            </Parameters>
            <Filter>
              <Parameters>
                <Parameter>
                  <Name>Dec</Name>
                  <VarName>deciles_eqDispy</VarName>
                  <Reform>false</Reform>
                </Parameter>
              </Parameters>
            </Filter>
          </Action>
          <Columns></Columns>
          <ReformColumns>
            <ReformColumn>
							<Name>Original income</Name>
              <StringFormat>0.00</StringFormat>
              <Action>
                <Parameters>
                  <Parameter>
                    <Name>MyBase</Name>
                    <VarName>eqOrigy</VarName>
                    <Reform>false</Reform>
                  </Parameter>
                  <Parameter>
                    <Name>MyCounter</Name>
                    <VarName>eqOrigy</VarName>
                  </Parameter>
                </Parameters>
              </Action>
						</ReformColumn>
            <ReformColumn>
              <Name>Public pensions</Name>
              <StringFormat>0.00</StringFormat>
              <Action>
                <Parameters>
                  <Parameter>
                    <Name>MyBase</Name>
                    <VarName>eqPen</VarName>
                    <Reform>false</Reform>
                  </Parameter>
                  <Parameter>
                    <Name>MyCounter</Name>
                    <VarName>eqPen</VarName>
                  </Parameter>
                </Parameters>
              </Action>
            </ReformColumn>
            <ReformColumn>
              <Name>Means-tested benefits</Name>
              <StringFormat>0.00</StringFormat>
              <Action>
                <Parameters>
                  <Parameter>
                    <Name>MyBase</Name>
                    <VarName>eqBenMt</VarName>
                    <Reform>false</Reform>
                  </Parameter>
                  <Parameter>
                    <Name>MyCounter</Name>
                    <VarName>eqBenMt</VarName>
                  </Parameter>
                </Parameters>
              </Action>
            </ReformColumn>
            <ReformColumn>
              <Name>Non means- tested benefits</Name>
              <StringFormat>0.00</StringFormat>
              <Action>
                <Parameters>
                  <Parameter>
                    <Name>MyBase</Name>
                    <VarName>eqBenNt</VarName>
                    <Reform>false</Reform>
                  </Parameter>
                  <Parameter>
                    <Name>MyCounter</Name>
                    <VarName>eqBenNt</VarName>
                  </Parameter>
                </Parameters>
              </Action>
            </ReformColumn>
            <ReformColumn>
              <Name>Employee SIC</Name>
              <StringFormat>0.00</StringFormat>
              <Action>
                <Parameters>
                  <Parameter>
                    <Name>MySign</Name>
                    <VarName>negative</VarName>
                  </Parameter>
                  <Parameter>
                    <Name>MyBase</Name>
                    <VarName>eqSicEe</VarName>
                    <Reform>false</Reform>
                  </Parameter>
                  <Parameter>
                    <Name>MyCounter</Name>
                    <VarName>eqSicEe</VarName>
                  </Parameter>
                </Parameters>
              </Action>
            </ReformColumn>
            <ReformColumn>
              <Name>Self-employed SIC</Name>
              <StringFormat>0.00</StringFormat>
              <Action>
                <Parameters>
                    <Parameter>
                      <Name>MySign</Name>
                      <VarName>negative</VarName>
                    </Parameter>
                  <Parameter>
                    <Name>MyBase</Name>
                    <VarName>eqSicSe</VarName>
                    <Reform>false</Reform>
                  </Parameter>
                  <Parameter>
                    <Name>MyCounter</Name>
                    <VarName>eqSicSe</VarName>
                  </Parameter>
                </Parameters>
              </Action>
            </ReformColumn>
            <ReformColumn>
              <Name>Other SIC</Name>
              <StringFormat>0.00</StringFormat>
              <Action>
                <Parameters>
                  <Parameter>
                    <Name>MySign</Name>
                    <VarName>negative</VarName>
                  </Parameter>
                  <Parameter>
                    <Name>MyBase</Name>
                    <VarName>eqSicOt</VarName>
                    <Reform>false</Reform>
                  </Parameter>
                  <Parameter>
                    <Name>MyCounter</Name>
                    <VarName>eqSicOt</VarName>
                  </Parameter>
                </Parameters>
              </Action>
            </ReformColumn>
            <ReformColumn>
              <Name>Direct taxes</Name>
              <StringFormat>0.00</StringFormat>
              <Action>
                <Parameters>
                  <Parameter>
                    <Name>MySign</Name>
                    <VarName>negative</VarName>
                  </Parameter>
                  <Parameter>
                    <Name>MyBase</Name>
                    <VarName>eqTax</VarName>
                    <Reform>false</Reform>
                  </Parameter>
                  <Parameter>
                    <Name>MyCounter</Name>
                    <VarName>eqTax</VarName>
                  </Parameter>
                </Parameters>
              </Action>
            </ReformColumn>
            <ReformColumn>
              <Name>Disposable income</Name>
              <StringFormat>0.00</StringFormat>
              <Action>
                <Parameters>
                  <Parameter>
                    <Name>MyBase</Name>
                    <VarName>eqDispy</VarName>
                    <Reform>false</Reform>
                  </Parameter>
                  <Parameter>
                    <Name>MyCounter</Name>
                    <VarName>eqDispy</VarName>
                  </Parameter>
                </Parameters>
              </Action>
            </ReformColumn>
          </ReformColumns>
					<Rows>
            <Row>
              <Name>Decile 1</Name>
              <Action>
                <Filter>
                  <FormulaString><![CDATA[OBS_VAR[@Dec] == 1]]></FormulaString>
                </Filter>
              </Action>
            </Row>
            <Row>
              <Name>Decile 2</Name>
              <Action>
                <Filter>
                  <FormulaString><![CDATA[OBS_VAR[@Dec] == 2]]></FormulaString>
                </Filter>
              </Action>
            </Row>
            <Row>
              <Name>Decile 3</Name>
              <Action>
                <Filter>
                  <FormulaString><![CDATA[OBS_VAR[@Dec] == 3]]></FormulaString>
                </Filter>
              </Action>
            </Row>
            <Row>
              <Name>Decile 4</Name>
              <Action>
                <Filter>
                  <FormulaString><![CDATA[OBS_VAR[@Dec] == 4]]></FormulaString>
                </Filter>
              </Action>
            </Row>
            <Row>
              <Name>Decile 5</Name>
              <Action>
                <Filter>
                  <FormulaString><![CDATA[OBS_VAR[@Dec] == 5]]></FormulaString>
                </Filter>
              </Action>
            </Row>
            <Row>
              <Name>Decile 6</Name>
              <Action>
                <Filter>
                  <FormulaString><![CDATA[OBS_VAR[@Dec] == 6]]></FormulaString>
                </Filter>
              </Action>
            </Row>
            <Row>
              <Name>Decile 7</Name>
              <Action>
                <Filter>
                  <FormulaString><![CDATA[OBS_VAR[@Dec] == 7]]></FormulaString>
                </Filter>
              </Action>
            </Row>
            <Row>
              <Name>Decile 8</Name>
              <Action>
                <Filter>
                  <FormulaString><![CDATA[OBS_VAR[@Dec] == 8]]></FormulaString>
                </Filter>
              </Action>
            </Row>
            <Row>
              <Name>Decile 9</Name>
              <Action>
                <Filter>
                  <FormulaString><![CDATA[OBS_VAR[@Dec] == 9]]></FormulaString>
                </Filter>
              </Action>
            </Row>
            <Row>
              <Name>Decile 10</Name>
              <Action>
                <Filter>
                  <FormulaString><![CDATA[OBS_VAR[@Dec] == 10]]></FormulaString>
                </Filter>
              </Action>
            </Row>
            <Row>
              <Name>Total</Name>
              <Strong>true</Strong>
            </Row>
          </Rows>
				</Table>
			</Tables>
		</Page>
	</Pages>
</Template>